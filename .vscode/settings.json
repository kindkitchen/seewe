{
  "deno.enable": true,
  "vue.server.hybridMode": true,
  "deno.enablePaths": [
    "api"
  ],
  "deno.disablePaths": [
    "src"
  ],
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    "class:list",
    "pt:root",
  ]
}