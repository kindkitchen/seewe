{
  "version": "3.0.1",
  "tasks": {
    "start": "deno run -A mod.ts"
  },
  "imports": {
    "@nik-kita/brickyard": "jsr:@nik-kita/brickyard@0.4.3",
    "@hono/swagger-ui": "npm:@hono/swagger-ui@^0.4.0",
    "@hono/zod-openapi": "npm:@hono/zod-openapi@^0.15.1",
    "@olli/kvdex": "jsr:@olli/kvdex@^1.0.2",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.0",
    "@std/http": "jsr:@std/http@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.2",
    "@wok/djwt": "jsr:@wok/djwt@^3.0.2",
    "hono": "npm:hono@^4.5.1",
    "zod": "npm:zod@^3.23.8"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": false
  },
  "unstable": [
    "kv"
  ],
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx",
    "strict": true,
    "useUnknownInCatchVariables": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true
  }
}
