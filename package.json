{
  "name": "seewe",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "api": "deno run -A --unstable-kv --watch api/mod.ts",
    "types:api": "curl http://localhost:3000/v1/json > api_version_one.json && npx openapi-typescript@latest ./api_version_one.json -o ./src/types/api_version_one.ts && npm run format",
    "dev": "vite",
    "brickyard:disable": "cp src/bricks.interceptor.example.ts src/bricks.interceptor.ts && cp api/bricks.interceptor.example.ts api/bricks.interceptor.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "npm run brickyard:disable && vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "npx eslint . --fix --ignore-path .gitignore && cd api && deno fmt",
    "format": "prettier --write src/",
    "see-bundle": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@formkit/vue": "^1.6.5",
    "@headlessui/tailwindcss": "^0.2.1",
    "@headlessui/vue": "^1.7.22",
    "@heroicons/vue": "^2.1.5",
    "@nik-kita/brickyard": "npm:@jsr/nik-kita__brickyard@^0.3.1",
    "@primevue/themes": "^4.0.4",
    "clsx": "^2.1.1",
    "md-editor-v3": "^4.18.0",
    "pinia": "^2.1.7",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.4",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vue3-google-signin": "^1.3.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@formkit/icons": "^1.6.5",
    "@playwright/test": "^1.44.1",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "openapi-typescript": "^7.3.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.7",
    "typescript": "~5.4.0",
    "unplugin-vue-router": "^0.10.2",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
